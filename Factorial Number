import java.util.Scanner;

public class Factorial {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println("enter a number ");
        int num = sc.nextInt();
        int val = 1;
        for (int i = num; i > 1; i--) {
            val = val * i;
        }
        System.out.println(val);

    }

}
